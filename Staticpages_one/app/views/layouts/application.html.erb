<!DOCTYPE html>
<html>
  <head>
    <title>StaticpagesOne</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
  </head>

  <body>
    <header class="navbar  navbar-default">
     <div class="container">

       <nav>
         <%= link_to image_tag("1.png", alt:"sample_log" ), static_pages_welcome_url,  id: "logo", class: "navbar-brand" %>
         <ul class="nav navbar-nav navbar-right">
           <li><%= link_to "Home",  static_pages_welcome_url %></li>
           <li><%= link_to "Help",   static_pages_help_url %></li>
           <li><%= link_to "Log in", '#' %></li>
         </ul>
       </nav>
     </div>
   </header>

    <%= yield %>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>

    <div class="text-center">
      <p>Esta es la pagina de <%=yield(:p)%></p>
    </div>
  </body>
</html>
